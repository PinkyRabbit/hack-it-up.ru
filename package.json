    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.10.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "express-validator": "^5.3.1",
    "helmet": "^3.18.0",
    "http-errors": "^1.7.2",
    "log4js": "^4.3.1",
    "markdown": "^0.5.0",
    "memorystore": "^1.6.1",
    "moment": "^2.29.2",
    "monk": "^7.0.0",
    "multer": "^1.4.1",
    "node-base64-image": "^1.0.6",
    "nodemailer": "^6.2.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
